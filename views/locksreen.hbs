<!-- Page -->
<div class="page vertical-align text-center" data-animsition-in="fade-in" data-animsition-out="fade-out">
    <div class="page-content vertical-align-middle animation-slide-top animation-duration-1">
        <div class="avatar avatar-lg">
            <img src="/{{user.local.photo}}" alt="...">
        </div>
        <p class="locked-user">{{user.local.name}}</p>
        <form method="post" action="/login">
            <input type="hidden" value="{{user.local.email}}" name="email">
            <div class="input-group">
                <input type="password" class="form-control last" id="inputPassword" name="password"
                       placeholder="Введите пароль">
                <span class="input-group-btn">
            <button type="submit" class="btn btn-primary"><i class="icon wb-unlock" aria-hidden="true"></i>
              <span class="sr-only">unLock</span>
            </button>
          </span>
            </div>
        </form>
        <p>Введите пароль для авторизации</p>
        <p><a href="/login">Или авторизуйтесь как другой пользователь</a></p>

        <footer class="page-copyright page-copyright-inverse">
            <p>WeWork</p>
        </footer>
    </div>
</div>
