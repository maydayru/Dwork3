<body class="animsition page-locked layout-full page-dark">
{{#if message}}
    <div class="alert dark alert-icon alert-danger alert-dismissible" role="alert">
        <i class="icon wb-close" aria-hidden="true"></i>{{message}}
    </div>
{{/if}}
