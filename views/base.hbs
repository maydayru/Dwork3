<script type="text/javascript">
    $(function() {
        $('input#srchbs').on('input', function () {
            var str = $(this).val().toLowerCase();
            if (str.length <= 2) {
                $('ul#srchitm li').show();
            }
            else {
                $('ul#srchitm li').each(function () {
                    if ($(this).text().toLowerCase().indexOf(str) < 0) {
                        $(this).hide();
                    }
                });
            }
        });
    });
</script>
